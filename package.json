{
  "name": "jbot-website",
  "version": "3.0.58",
  "description": "Personal website for Joey Guerra",
  "main": "server.mjs",
  "dependencies": {
    "@hubot-friends/hubot-discord": "^4.1.0",
    "body-parser": "^2.2.1",
    "dijkstrajs": "^1.0.3",
    "discord.js": "^14.22.1",
    "html-to-pdfmake": "^2.5.31",
    "hubot": "^13.2.0",
    "jimp": "^1.6.0",
    "jose": "^6.1.0",
    "jsdom": "^27.0.0",
    "juphjacs": "github:joeyguerra/juphjacs",
    "markdown-it": "^14.1.0",
    "markdown-it-meta": "^0.0.1",
    "morphdom": "^2.7.7",
    "openai": "^6.1.0",
    "pdfmake": "^0.2.20",
    "pino-pretty": "^13.1.1",
    "pngjs": "^7.0.0",
    "socket.io": "^4.8.1",
    "tedious": "^18.6.1",
    "xml2js": "^0.6.2"
  },
  "scripts": {
    "test": "node --test",
    "start": "LOG_LEVEL=debug node --env-file-if-exists=.env ./node_modules/hubot/bin/Hubot.mjs --disable-httpd -a @hubot-friends/hubot-discord -n jbot",
    "dev": "LOG_LEVEL=debug node --env-file-if-exists=.env --watch Server.mjs",
    "jbot": "LOG_LEVEL=debug node --env-file-if-exists=.env --watch ./node_modules/hubot/bin/Hubot.mjs --disable-httpd -a @hubot-friends/hubot-discord -n jbot",
    "docker-build": "./docker-build-k3d.sh",
    "local-deploy": "kubectl config use-context ${KUBE_CONTEXT:-k3d-local} && kubectl apply -f charts/web/deployment.yaml -n ${KUBE_NAMESPACE:-default}",
    "push": "node --run docker-build && node --run local-deploy",
    "signal:check": "node tools/signal-check.mjs --staged",
    "signal:check:strict": "node tools/signal-check.mjs --staged --strict=85 --warn=90"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeyguerra/joeyguerra"
  },
  "keywords": [
    "joey",
    "guerra",
    "personal",
    "site"
  ],
  "author": "Joey Guerra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joeyguerra/joeyguerra/issues"
  },
  "homepage": "https://github.com/joeyguerra/joeyguerra#readme"
}
